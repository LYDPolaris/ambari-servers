{
      "name": "default",
      "description": "default quick links configuration",
      "configuration": {
        "protocol": {
          "type":"https",
          "checks":[
            {
              "property":"HTTP_ONLY",
              "desired":"HTTPS_ONLY",
              "site":"elastic-config"
            }
          ]
        },
        "links": [
          {
            "name": "elasticsearch_head",
            "label": "Elasticsearch Head",
            "component_name": "ELASTICSEARCH_SQL",
            "requires_user_name": "false",
            "url":"%@://%@:%@/head/",
            "port":{
              "http_property": "elastic_web_plugin_server_port",
              "http_default_port": "7070",
              "https_property": "elastic_web_plugin_server_port",
              "https_default_port": "7070",
              "regex": "^(\\d+)$",
              "site": "site-server-config"
            }
          },
          {
            "name": "elasticsearch_sql",
            "label": "Elasticsearch Sql",
            "component_name": "ELASTICSEARCH_SQL",
            "requires_user_name": "false",
            "url":"%@://%@:%@/esql/",
            "port":{
              "http_property": "elastic_web_plugin_server_port",
              "http_default_port": "7070",
              "https_property": "elastic_web_plugin_server_port",
              "https_default_port": "7070",
              "regex": "^(\\d+)$",
              "site": "site-server-config"
            }
          },
          {
            "name": "elasticsearch_bigdesk",
            "label": "Elasticsearch Bigdesk",
            "component_name": "ELASTICSEARCH_SQL",
            "requires_user_name": "false",
            "url":"%@://%@:%@/bigdesk/",
            "port":{
              "http_property": "elastic_web_plugin_server_port",
              "http_default_port": "7070",
              "https_property": "elastic_web_plugin_server_port",
              "https_default_port": "7070",
              "regex": "^(\\d+)$",
              "site": "site-server-config"
            }
          }
        ]
      }
}